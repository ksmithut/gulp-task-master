  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "expect.js": "^0.3.1",
    "gulp": "^3.8.10",
    "istanbul": "^0.3.5",
    "mocha": "^2.0.1",
    "mocha-lcov-reporter": "0.0.1"
  },
